{
  "name": "@pinjs/gem-cli",
  "version": "0.0.5",
  "description": "⚡️ Gempages Generation CLI Experience",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pin705/gem-cli"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/gem-cli.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "gem-cli": "./bin/gem-cli.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev:prepare": "unbuild --stub",
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "gem-cli": "node ./bin/gem-cli.mjs",
    "gem-cli-bun": "bun --bun ./bin/gem-cli.mjs",
    "prepack": "unbuild",
    "test": "pnpm lint && pnpm test:types && pnpm build && pnpm test:dist",
    "test:types": "tsc --noEmit",
    "release": "changelogen --release && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "changelogen": "^0.5.5",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "eslint": "^9.2.0",
    "eslint-config-unjs": "^0.3.2",
    "unbuild": "^2.0.0",
    "@types/prompts": "^2.4.9",
    "node-html-parser": "^6.1.13"
  },
  "engines": {
    "node": "^16.10.0 || >=18.0.0"
  }
}
